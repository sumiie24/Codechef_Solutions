#include <bits/stdc++.h>
using namespace std;

int main()
{
	long long int t,n,x,s,i=0;

	scanf("%d",&t);
	while(t--){

    scanf("%d%d%d",&n,&x,&s);
    long long int a[999999]={0};

    a[x-1]=1;


     for(i=0;i<s;i++){
        int p,q;
        scanf("%d%d",&p,&q);
            swap(a[p-1],a[q-1]);


        }

    for(i=0;i<n;i++){
            if(a[i]==1){
                    printf("%d\n",i+1);
                }
             }
	}
	return 0;
}
